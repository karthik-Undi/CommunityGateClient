@model CommunityGateClient.Models.DashBoardPosts

@{
    ViewData["Title"] = "DeleteNotice";
    Layout = "~/Views/Shared/_ResidentLayout.cshtml";
}
<div class="col-md-6">
    <div class="card card-primary">
        <div class="card-header">
            <h3 class="card-title">Delete Notice</h3>

        </div>
        <div class="card-body">
            <div class="form-group">

                <h4>Are you sure you want to delete this Post?</h4><h5 style="color:red"> You cannot undo this action</h5>
                <br /><br />
                <form asp-action="DeleteNotice">
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </form>
            </div>
        </div>
    </div>
</div>

